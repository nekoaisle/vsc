{
    "name": "cpss-wizard",
    "displayName": "Cpss Wizard",
    "description": "テンプレートから初期ソースを作成",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:nekoaisle.cpssWizard"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
                "command": "nekoaisle.cpssWizard",
                "title": "Cpss Wizard"
        }],
        "keybindings": [{
            "command": "nekoaisle.cpssWizard",
            "key": "escape p"
        }],
        "configuration": {
            "type": "object",
            "title": "nekoaisle: CPSS Wizerd configurations",
            "properties": {
                "cpssWizard.wizard": {
                    "type": "string",
                    "default": "",
                    "description": "Wizardファイル名"
                },
                "cpssWizard.templateDir": {
                    "type": "string",
                    "default": "",
                    "description": "標準テンプレート格納ディレクトリ"
                },
                "cpssWizard.sqlDir": {
                    "type": "string",
                    "default": "~/",
                    "description": "SQLファイル格納ディレクトリ"
                },
                "cpssWizard.php": {
                    "type": "string",
                    "default": "/usr/bin/php",
                    "description": "php格納ディレクトリ"
                },
                "cpssWizard.outFile": {
                    "type": "string",
                    "default": "php://stdout",
                    "description": "出力ファイル名"
                },
                "cpssWizard.author": {
                    "type": "string",
                    "default": "木屋善夫",
                    "description": "著者名"
                },
                "cpssWizard.charset": {
                    "type": "string",
                    "default": "Shift_JIS",
                    "description": "文字コード"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
