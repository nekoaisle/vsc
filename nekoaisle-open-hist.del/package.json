{
	"name": "nekoaisle-open-hist",
	"displayName": "Open History",
	"description": "Open file from history",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nekoaisle.openHist",
		"onCommand:nekoaisle.openHistCompensateDate",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nekoaisle.openHist",
				"title": "nekoaisle.openHist 編集したことのあるファイルを開く"
			},{
				"command": "nekoaisle.openHistCompensateDate",
				"title": "nekoaisle.openHistCompensateDate ファイル編集履歴の修正"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.6.3",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"tsutils": "^3.17.1"
	}
}
