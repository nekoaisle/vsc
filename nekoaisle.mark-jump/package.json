{
    "name": "markjump",
    "displayName": "MarkJump",
    "description": "Mark the cursor position and jump to the marked position.",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:markjump.mark",
        "onCommand:markjump.jump"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "markjump.mark",
                "title": "Mark the cursor position"
            },
            {
                "command": "markjump.jump",
                "title": "Mark the cursor position"
            },
            {
                "command": "markjump.rejump",
                "title": "Return to the last jump position"
            }
        ],
        "keybindings": [
            {
                "command": "markjump.mark",
                "key": "escape m"
            },
            {
                "command": "markjump.jump",
                "key": "escape j"
            },
            {
                "command": "markjump.rejump",
                "key": "escape r"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
