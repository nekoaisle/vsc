{
    "name": "encode",
    "displayName": "Encode",
    "description": "Encode the selection URL or HTML.",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:nekoaisle.encode",
        "onCommand:nekoaisle.encloseInSingleQuotation",
        "onCommand:nekoaisle.encloseInDoubleQuotation"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "nekoaisle.encode",
            "title": "Encode the selection URL or HTML."
        }],
        "keybindings": [{
            "command": "nekoaisle.encode",
            "key": "escape e"
        },{
            "command": "nekoaisle.encloseInSingleQuotation",
            "key": "ctrl+7"
        },{
            "command": "nekoaisle.encloseInDoubleQuotation",
            "key": "ctrl+2"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
