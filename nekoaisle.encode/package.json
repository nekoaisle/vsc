            },
            {
                "command": "nekoaisle.decodeUrl",
                "title": "nekoaisle.decodeUrl Url デコード"
            },
            {
                "command": "nekoaisle.decodeBase64",
                "title": "nekoaisle.decodeBase64 Base64 デコード"
            },
            {
                "command": "nekoaisle.decodeCString",
                "title": "nekoaisle.decodeCString C-String デコード"
            },
            {
                "command": "nekoaisle.decodePreg",
