            },
            {
                "command": "nekoaisle.encloseDoubleCurlyBracket",
                "title": "nekoaisle.encloseDoubleCurlyBracket {} で単語または選択範囲を括る/外す"
            },
            {
                "command": "nekoaisle.encloseCComment",
                "title": "nekoaisle.encloseCComment /**/ で単語または選択範囲を括る/外す"
            },
            {
                "command": "nekoaisle.encloseHtmlComment",
                "title": "nekoaisle.encloseHtmlComment HTML コメント/外す"
