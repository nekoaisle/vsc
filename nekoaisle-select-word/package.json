{
    "name": "select-word",
    "displayName": "Select Word",
    "description": "カーソル位置の単語を選択",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:nekoaisle.selectWord",
        "onCommand:nekoaisle.addCursor",
        "onCommand:nekoaisle.delCursor"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "nekoaisle.selectWord",
                "title": "Select Word"
            },
            {
                "command": "nekoaisle.addCursor",
                "title": "Add cursor"
            },
            {
                "command": "nekoaisle.delCursor",
                "title": "Add cursor"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^6.2.1",
        "typescript": "^2.0.3",
        "vscode": "^1.1.36"
    }
}
