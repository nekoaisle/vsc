{
    "name": "open-file",
    "displayName": "Open File",
    "description": "新規・既存・関連ファイルを開く",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:nekoaisle.openFile",
        "onCommand:nekoaisle.openNew",
        "onCommand:nekoaisle.openRelated"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "nekoaisle.openFile",
            "title": "Open existing file"
        },{
            "command": "nekoaisle.openRelated",
            "title": "Open related file"
        },{
            "command": "nekoaisle.openNew",
            "title": "Open new file"
        }],
        "keybindings": [{
            "command": "nekoaisle.openFile",
            "key": "escape f"
        },{
            "command": "nekoaisle.openRelated",
            "key": "escape h"
        },{
            "command": "nekoaisle.openNew",
            "key": "escape n"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
