			"\t'Permission' => PERMISSION_${2|READ,MODIFY,DELETE|},",
			"\t// 'RightCommand' => '',",
			"\t// 'RightValue'   => NULL,",
			"\t'LeftCommand' => '!@$1 delete',",
			"\t'LeftValue'   => TRUE,",
			"];"
