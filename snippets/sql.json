{
/*
	// Place your snippets for SQL here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
*/
	"V_ACCOUNT_ID": {
		"prefix": "V_ACCOUNT_ID",
		"body": [
			"V_ACCOUNT_ID    VARCHAR( 64) NOT NULL DEFAULT '' -- V_ACCOUNT.V_ID"
		],
		"description": "V_ACCOUNT_ID    VARCHAR( 64) NOT NULL DEFAULT '' -- V_ACCOUNT.V_ID"
	},
	"CHAR": {
		"prefix": "CHAR   (  3) NOT NULL DEFAULT '' -- ",
		"body": [
			"CHAR   (${1:  3}) NOT NULL DEFAULT '' -- $2"
		],
		"description": "CHAR   (  3) NOT NULL DEFAULT '' -- "
	},
	"VARCHAR( 64) NOT NULL DEFAULT '' --": {
		"prefix": "VARCHAR( 64) NOT NULL DEFAULT '' --",
		"body": [
			"VARCHAR(${1: 64}) NOT NULL DEFAULT '' -- $2"
		],
		"description": "VARCHAR( 64) NOT NULL DEFAULT '' --"
	},
	"VARCHAR(255) NOT NULL DEFAULT '' -- ": {
		"prefix": "VARCHAR(255) NOT NULL DEFAULT '' -- ",
		"body": [
			"VARCHAR(${1:255}) NOT NULL DEFAULT '' -- $2"
		],
		"description": "VARCHAR(255) NOT NULL DEFAULT '' -- "
	},
	"NUMERIC(  9) NOT NULL DEFAULT 0  --": {
		"prefix": "NUMERIC(  9) NOT NULL DEFAULT 0  -- ",
		"body": [
			"NUMERIC(  9) NOT NULL DEFAULT 0  -- $1"
		],
		"description": "NUMERIC(  9) NOT NULL DEFAULT 0  --"
	}
}