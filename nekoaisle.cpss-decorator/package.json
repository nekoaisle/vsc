{
    "name": "cpss-decorator",
    "displayName": "CPSS Decorator",
    "description": "Highlight for CPSS",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "nekoaisle.cpssDecoratorRefresh",
                "title": "nekoaisle.cpssDecoratorRefresh デザインのリロード"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "CPSS用HTMLデコレータ",
            "properties": {
                "cpssDecorator.dataDir": {
                    "type": "string",
                    "default": "",
                    "description": "デザイン/ハイライトファイルが格納されているディレクトリを指定してください。"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.38.0",
        "mocha": "^10.2.0",
        "typescript": "^3.3.1",
        "vscode-test": "^1.2.0"
    },
    "dependencies": {}
}
