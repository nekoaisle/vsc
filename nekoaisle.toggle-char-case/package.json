{
    "name": "togglecharcase",
    "displayName": "ToggleCharCase",
    "description": "Toggle the case of the character at the cursor position.",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.toggleCharCase"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
                "command": "extension.toggleCharCase",
                "title": "Toggle the case of the character"
        }],
        "keybindings": [{
            "command": "extension.toggleCharCase",
            "key": "ctrl ["
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
