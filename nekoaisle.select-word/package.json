{
    "name": "selectword",
    "displayName": "SelectWord",
    "description": "カーソル位置の単語を選択",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:nekoaisle.selectWord"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "nekoaisle.selectWord",
            "title": "Select Word"
        }],
        "keybindings": [{
            "command": "nekoaisle.selectWord",
            "key": "shift+enter"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
