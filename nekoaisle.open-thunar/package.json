{
    "name": "openthunar",
    "displayName": "OpenThunar",
    "description": "現在のファイルが格納されているディレクトリをThunarで開く",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:nekoaisle.openThunar"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "nekoaisle.openThunar",
                "title": "Open Thunar"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Open thunar",
            "properties": {
                "nekoaisle.filer": {
                    "type": "string",
                    "default": "",
                    "description": "ファイラーのファイル名を指定してください。"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
