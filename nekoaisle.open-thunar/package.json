{
    "name": "openthunar",
    "displayName": "OpenThunar",
    "description": "Open the folder containing the file you are editing in Thunar.",
    "version": "0.0.1",
    "publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:openThunar.open"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "openThunar.open",
                "title": "Open Thunar"
            }
        ],
        "keybindings": [{
            "command": "openThunar.open",
            "key": "escape x"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
