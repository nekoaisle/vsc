// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
	{"key": "f1"        ,"command": "nekoaisle.openHelp"},
	{"key": "f9"        ,"command": "editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly"},
	{"key": "f10"       ,"command": "nekoaisle.toggleTab", "when": "editorTextFocus"},

	{"key": "ctrl+enter","command": "-editor.action.insertLineAfter","when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+enter","command": "removeSecondaryCursors", "when": "editorHasMultipleSelections && editorTextFocus"},
	{"key": "ctrl+up"   ,"command": "nekoaisle.delCursor", "when": "editorTextFocus"},
	{"key": "ctrl+down" ,"command": "nekoaisle.addCursor", "when": "editorTextFocus"},
	{"key": "ctrl+["    ,"command": "-editor.action.outdentLines","when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+["    ,"command": "nekoaisle.encloseCurlyBracket", "when": "editorTextFocus"},
	{"key": "ctrl+]"    ,"command": "-editor.action.indentLines","when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+]"    ,"command": "nekoaisle.toggleCharCase", "when": "editorTextFocus"},
	{"key": "ctrl+2"    ,"command": "nekoaisle.encloseInDoubleQuotation", "when": "editorTextFocus"},
	{"key": "ctrl+3"    ,"command": "editor.emmet.action.evaluateMathExpression"},
	{"key": "ctrl+7"    ,"command": "nekoaisle.encloseInSingleQuotation", "when": "editorTextFocus"},
	{"key": "ctrl+e"    ,"command": "deleteAllRight"},
	{"key": "ctrl+f"    ,"command": "closeFindWidget", "when": "editorFocus && findInputFocussed && findWidgetVisible"},
	{"key": "ctrl+j"    ,"command": "nekoaisle.markjumpJump", "when": "editorTextFocus"},
	{"key": "ctrl+m"    ,"command": "-editor.action.toggleTabFocusMode"},
	{"key": "ctrl+m"    ,"command": "nekoaisle.markjumpMark", "when": "editorTextFocus"},
	{"key": "ctrl+n"    ,"command": "nekoaisle.openNew"},
	{"key": "ctrl+o"    ,"command": "-workbench.action.files.openFile"},
	{"key": "ctrl+q"    ,"command": "-workbench.action.quit"},
	{"key": "ctrl+q"    ,"command": "nekoaisle.commandMenu", "when": "editorTextFocus"},
	{"key": "ctrl+l"    ,"command": "nekoaisle.insertCode.pinfoBase", "when": "editorTextFocus && !editorReadonly"},

	{"key": "ctrl+shift+enter","command": "type", "args": { "text": "<br />" }, "when": "editorTextFocus && !editorReadonly && editorLangId == 'html'"},
	{"key": "ctrl+shift+space","command": "type", "args": { "text": "&nbsp;" }, "when": "editorTextFocus && !editorReadonly && editorLangId == 'html'"},
	{"key": "ctrl+shift+,"    ,"command": "type", "args": { "text": "&lt;" }, "when": "editorTextFocus && !editorReadonly && editorLangId == 'html'"},
	{"key": "ctrl+shift+."    ,"command": "type", "args": { "text": "&gt;" }, "when": "editorTextFocus && !editorReadonly && editorLangId == 'html'"},
	{"key": "ctrl+shift+/"    ,"command": "editor.action.blockComment","when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+shift+["    ,"command": "-editor.fold","when": "editorTextFocus"},
	{"key": "ctrl+shift+["    ,"command": "nekoaisle.encloseDoubleCurlyBracket", "when": "editorTextFocus"},
	{"key": "ctrl+shift+]"    ,"command": "-editor.unfold","when": "editorTextFocus"},
	{"key": "ctrl+shift+]"    ,"command": "nekoaisle.toggleWordCase", "when": "editorTextFocus"},
	{"key": "ctrl+shift+6"    ,"command": "type", "args": { "text": "&amp;" }, "when": "editorTextFocus && !editorReadonly && editorLangId == 'html'"},
	{"key": "ctrl+shift+l"    ,"command": "editor.action.selectHighlights", "when": "editorFocus"},

	{"key": "ctrl+w /"        ,"command": "editor.action.formatDocument", "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"},
	{"key": "ctrl+w ["        ,"command": "editor.emmet.action.matchTag"},
	{"key": "ctrl+w \\"       ,"command": "workbench.action.editor.changeEncoding", "when": "editorTextFocus"},
	{"key": "ctrl+w ]"        ,"command": "editor.action.jumpToBracket", "when": "editorTextFocus"},
	{"key": "ctrl+w 1"        ,"command": "nekoaisle.jumpToLineNumber1", "when": "editorTextFocus"},
	{"key": "ctrl+w 2"        ,"command": "nekoaisle.jumpToLineNumber2", "when": "editorTextFocus"},
	{"key": "ctrl+w 3"        ,"command": "nekoaisle.jumpToLineNumber3", "when": "editorTextFocus"},
	{"key": "ctrl+w 4"        ,"command": "nekoaisle.jumpToLineNumber4", "when": "editorTextFocus"},
	{"key": "ctrl+w 5"        ,"command": "nekoaisle.jumpToLineNumber5", "when": "editorTextFocus"},
	{"key": "ctrl+w 6"        ,"command": "nekoaisle.jumpToLineNumber6", "when": "editorTextFocus"},
	{"key": "ctrl+w 7"        ,"command": "nekoaisle.jumpToLineNumber7", "when": "editorTextFocus"},
	{"key": "ctrl+w 8"        ,"command": "nekoaisle.jumpToLineNumber8", "when": "editorTextFocus"},
	{"key": "ctrl+w 9"        ,"command": "nekoaisle.jumpToLineNumber9", "when": "editorTextFocus"},
	{"key": "ctrl+w c"        ,"command": "workbench.action.terminal.focus"},
	{"key": "ctrl+w e"        ,"command": "nekoaisle.encode", "when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+w f"        ,"command": "workbench.action.files.openFile"},
//	{"key": "ctrl+w g"        ,"command": "nekoaisle.grepSjis", "when": "editorTextFocus"},
	{"key": "ctrl+w h"        ,"command": "nekoaisle.openRelated", "when": "editorTextFocus"},
	{"key": "ctrl+w i"        ,"command": "nekoaisle.insertCode", "when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+w k"        ,"command": "nekoaisle.openHist", "when": "editorTextFocus"},
	{"key": "ctrl+w l"        ,"command": "nekoaisle.insertFile", "when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+w p"        ,"command": "nekoaisle.cpssWizard"},
	{"key": "ctrl+w q"        ,"command": "workbench.action.closeActiveEditor","when": "editorTextFocus"},
	{"key": "ctrl+w r"        ,"command": "nekoaisle.markjumpReturn", "when": "editorTextFocus"},
	{"key": "ctrl+w s"        ,"command": "editor.action.sortLinesAscending"},
	{"key": "ctrl+w t"        ,"command": "workbench.action.tasks.runTask"},
	{"key": "ctrl+w w"        ,"command": "nekoaisle.openTemp"},
	{"key": "ctrl+w x"        ,"command": "nekoaisle.openFiler"},

	// 以下はエクスプローラ
	{"key": "shift+delete","command": "-deleteFile","when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceMoveableToTrash && !explorerResourceReadonly && !inputFocus"},

	{"key": "ctrl+insert" ,"command": "filesExplorer.copy" ,"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"},
	{"key": "ctrl+c"      ,"command": "filesExplorer.copy" ,"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"},
	{"key": "shift+insert","command": "filesExplorer.paste","when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"},
	{"key": "ctrl+v"      ,"command": "filesExplorer.paste","when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"},
	{"key": "shift+delete","command": "filesExplorer.cut"  ,"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !explorerResourceIsRoot && !inputFocus"},
	{"key": "ctrl+x"      ,"command": "filesExplorer.cut"  ,"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !explorerResourceIsRoot && !inputFocus"},

	// 以下自動挿入（最初の1つは挿入時に壊される…）
	{"key": "shift+enter"     ,"command": "nekoaisle.selectWord", "when": "editorTextFocus"},
	{
		"key": "ctrl+alt+up",
		"command": "workbench.files.action.collapseExplorerFolders"
	},
	{
		"key": "ctrl+shift+numpad_multiply",
		"command": "workbench.action.togglePanelPosition"
	},
]